{
  "name": "@lando/vuepress-docs-theme",
  "description": "A VuePress 2 Theme for Lando Doc Based Sites",
  "version": "0.1.2",
  "author": "John Ouellet @labboy0276",
  "license": "GPL-3.0",
  "repository": "github:lando/vuepress-docs-theme",
  "bugs": "https://github.com/lando/vuepress-docs-theme/issues/new/choose/",
  "homepage": "https://github.com/lando/vuepress-docs-theme",
  "keywords": [
    "lando",
    "vuepress"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs --dest _site",
    "docs:lint": "eslint -c docs/.eslintrc.json --quiet docs/.vuepress",
    "lint": "eslint --quiet .",
    "release": "bump --prompt --tag --all --push",
    "test": "yarn lint"
  },
  "dependencies": {
    "js-yaml": "^3.4.6",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.16.0",
    "@vuepress/plugin-docsearch": "^2.0.0-beta.27",
    "@vuepress/plugin-palette": "^2.0.0-alpha.26",
    "@vuepress/plugin-register-components": "^2.0.0-beta.27",
    "eslint": "^7.32.0",
    "eslint-config-google": "^0.9.1",
    "eslint-plugin-vue": "^8.0.3",
    "version-bump-prompt": "^4",
    "vuepress": "^2.0.0-beta.27"
  }
}